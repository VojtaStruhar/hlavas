[sub_resource type="GDScript" id="GDScript_kry3j"]
resource_name = "debug_ui"
script/source = "extends CanvasLayer

@onready var people_label: Label = %PeopleLabel
@onready var people_inside_label: Label = %PeopleInsideLabel
@onready var fps_label: Label = %FPSLabel
@onready var background_check_box: CheckBox = %BackgroundCheckBox
@onready var colored_people_checkbox: CheckBox = %ColoredPeopleCheckbox


@onready var bw_image = $\"../UnderlyingPhoto/BrnoBlackAndWhite\"
@onready var brno_colored: Sprite2D = $\"../UnderlyingPhoto/BrnoColored\"

const BRNAK = preload(\"res://Scenes/brnak.tscn\")

func _ready() -> void:
	visible = false
	brno_colored.modulate.a = 1 if Settings.background_photo else 0
	
	background_check_box.button_pressed = Settings.background_photo
	colored_people_checkbox.button_pressed = Settings.colored_people
	
	background_check_box.toggled.connect(_on_background_check_box_toggled)
	colored_people_checkbox.toggled.connect(_on_colored_people_checkbox_toggled)

func _process(delta: float) -> void:
	if Input.is_action_just_pressed(\"toggle_options\"):
		visible = not visible
	
	people_label.text = \"People: \" + str(Stats.people_outside)
	people_inside_label.text = \"In shops: \" + str(Stats.people_inside)
	fps_label.text = \"FPS: \" + str(Engine.get_frames_per_second())


func _on_background_check_box_toggled(toggled_on: bool) -> void:
	Settings.background_photo = toggled_on
	var t = create_tween()
	t.tween_property(brno_colored, \"modulate:a\", 1 if toggled_on else 0, 1.0).set_ease(Tween.EASE_IN_OUT)


func _on_colored_people_checkbox_toggled(toggled_on: bool) -> void:
	Settings.colored_people = toggled_on
"

[sub_resource type="GDScript" id="GDScript_ynf5e"]
resource_name = "BackgroundToggleTimer"
script/source = "extends Timer

var bg: Sprite2D
var bg_visible: bool = false

func _ready() -> void:
	bg = get_parent()
	bg.modulate.a = 0
	timeout.connect(_toggle_bg)

func _toggle_bg() -> void:
	bg_visible = not bg_visible
	var t = create_tween()
	t.tween_property(bg, \"modulate:a\", 0.15 if bg_visible else 0, 5.0).set_ease(Tween.EASE_IN_OUT)
"

